<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <service id="AIDemoData\Repository\TaxRepository" class="AIDemoData\Repository\TaxRepository">
            <argument type="service" id="tax.repository"/>
        </service>

        <service id="AIDemoData\Repository\SalesChannelRepository" class="AIDemoData\Repository\SalesChannelRepository">
            <argument type="service" id="sales_channel.repository"/>
        </service>

        <service id="AIDemoData\Repository\CurrencyRepository" class="AIDemoData\Repository\CurrencyRepository">
            <argument type="service" id="currency.repository"/>
        </service>

        <service id="AIDemoData\Repository\CategoryRepository" class="AIDemoData\Repository\CategoryRepository">
            <argument type="service" id="category.repository"/>
        </service>

        <service id="AIDemoData\Repository\ProductRepository" class="AIDemoData\Repository\ProductRepository">
            <argument type="service" id="product.repository"/>
        </service>

        <service id="AIDemoData\Repository\PropertyRepository" class="AIDemoData\Repository\PropertyRepository">
            <argument type="service" id="property_group_option.repository"/>
        </service>

    </services>
</container>